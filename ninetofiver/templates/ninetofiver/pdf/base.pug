- load static
doctype 5
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
    meta(http-equiv='x-ua-compatible' content='ie=edge')

    title
      block title

    base(href='/')
    meta(name="apple-mobile-web-app-capable" content="yes")
    meta(name="apple-mobile-web-app-status-bar-style" content="black")
    meta(name="mobile-web-app-capable" content="yes")
    link(
      href='{% static "ninetofiver/img/touch_icon_128.png" %}'
      rel="apple-touch-icon-precomposed"
      sizes="128x128"
    )
    link(
      href='{% static "ninetofiver/img/touch_icon_192.png" %}'
      rel="apple-touch-icon-precomposed"
      sizes="192x192"
    )
    link(
      href='{% static "ninetofiver/img/touch_icon_128.png" %}'
      rel="shortcut icon"
      sizes="128x128"
    )
    link(
      href='{% static "ninetofiver/img/touch_icon_192.png" %}'
      rel="shortcut icon"
      sizes="192x192"
    )
    link(
      href='{% static "ninetofiver/img/favicon.ico" %}'
      rel="shortcut icon"
      type="image/x-icon"
    )

    link(
      href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    )
    link(
      href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
      rel="stylesheet"
      integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN"
      crossorigin="anonymous"
    )
    link(
      rel='stylesheet'
      href='{{ settings.BASE_URL }}{% static "ninetofiver/css/layout.css" %}'
    )
    link(
      rel='stylesheet'
      href='{{ settings.BASE_URL }}{% static "ninetofiver/css/pdf.css" %}'
    )
  body
    block body
